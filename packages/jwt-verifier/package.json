{
  "name": "@serverless-jwt/jwt-verifier",
  "version": "0.1.4",
  "description": "JWT verification for Serverless environments",
  "author": "Sandrino Di Mattia",
  "homepage": "https://github.com/sandrinodimattia/serverless-jwt#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {

{
  "name": "@serverless-jwt/next",
  "version": "0.1.3",
  "description": "JWT verification for Next.js API Routes",
  "author": "Sandrino Di Mattia",
  "homepage": "https://github.com/sandrinodimattia/serverless-jwt#readme",
